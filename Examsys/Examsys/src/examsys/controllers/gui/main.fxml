<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="examsys.controllers.MainController">
   <children>
      <MenuBar VBox.vgrow="ALWAYS">
        <menus>
          <Menu fx:id="exam_menu" mnemonicParsing="false" text="Exam">
               <items>
                  <MenuItem fx:id="my_exams" mnemonicParsing="false" onAction="#myExams" text="My Exams" />
               </items></Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <VBox fx:id="main_vbox" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
   </children>
    
</VBox>
